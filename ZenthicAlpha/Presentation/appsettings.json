{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "ApplicationDbContext": "Server=[serverName]; Initial Catalog=[databaseName]; User ID=[userName]; Password=[userPassword];"
  },
  "CacheSettings": {
    "ConnectionString": "[host]:[port]",
    "AbsoluteLifetime": "000:00:15:00.000",
    "SlidingLifetime": "000:00:05:00.000"
  },
  "HashingSettings": {
    "Algorithm": "SHA512",
    "Iterations": 10000
  },
  "JwtSettings": {
    "Secret": "[guid]",
    "Issuer": "[host]",
    "RefreshTokenLifetime": "305:00:00:00.000",
    "TokenLifetime": "000:12:00:00.000"
  },
  "BackgroundTaskSettings": {
    "TimeInterval": "000:00:00:30.000"
  },
  "SmtpSettings": {
    "Server": "[serverName]",
    "Port": 0,
    "FromEmail": "no-reply@localhost.com",
    "FromName": "localhost",
    "EnableSsl": true,
    "User": "[user]",
    "Password": "[password]"
  }
}
